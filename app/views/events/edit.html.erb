<div class="container">
	<div class="text-center">
		<h1>Edit Event</h1>
	</div>

		<%= bootstrap_form_for(@events, layout: :horizontal) do |t| %>
			<p><%= t.text_field :date %></p>
			<p><%= t.text_field :alcohol_served %></p>
		
		<div class="text-center">		
				<%= submit_tag "Confirm" %>
		</div>
			<% end %>
		
</div>